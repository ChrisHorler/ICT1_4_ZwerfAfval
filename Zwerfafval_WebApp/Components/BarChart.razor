@using MudBlazor



	<Mudchart 
		ChartType="ChartType.Bar"
		ChartSeries="@barseries"
		XAxisLabels="@barChartXAxisLabels"
		width="650" Height="350px">
	</Mudchart>


    @code {
	[Parameter]
	public List<ChartSeries> barseries { get; set; }

	[Parameter]
	public string[] barChartXAxisLabels { get; set; }
    }
